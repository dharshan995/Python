# Get the value of n from the user
n = int(input("Enter the value of n: "))

# Initialize variables
count = 0
num = 1

# Loop until we find the nth number which is both a square and a cube
while True:
    # Check if the number is both a square and a cube
    if num ** (1/2) == int(num ** (1/2)) and num ** (1/3) == int(num ** (1/3)):
        count += 1
    # Check if we have found the nth number
    if count == n:
        print(f"The {n}-th number which is both a square and a cube is:", num)
        break
    num += 1
