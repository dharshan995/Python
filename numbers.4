# Get the value of n from the user
n = int(input("Enter the value of n: "))

# Initialize variables
sum_of_numbers = 0
count = 1

# Use a while loop to iterate until count reaches n
while count <= n:
    # Add the current value of count to the sum
    sum_of_numbers += count
    # Increment count
    count += 1

