array = []


n = int(input("Enter the number of elements: "))
print("Enter the sorted elements:")
for i in range(n):
    num = int(input())
    array.append(num)
last_duplicate_index = -1
last_duplicate_element = None
for i in range(1, n):
    if array[i] == array[i - 1]:
        last_duplicate_index = i
        last_duplicate_element = array[i]


if last_duplicate_index != -1:
    print("Index of last duplicate element:", last_duplicate_index)
    print("Last duplicate element:", last_duplicate_element)
else:
    print("No duplicate element found.")
